<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<AssemblyName>Dagger.Thunk</AssemblyName>
		<RootNamespace>Dagger.Thunk</RootNamespace>
		<IsPackable>false</IsPackable>
	</PropertyGroup>

	<ItemGroup>
		<Reference Include="Dagger.SDK">
			<HintPath>Libraries/Dagger.SDK.dll</HintPath>
		</Reference>
		<Compile Remove="Libraries\**" />
		<EmbeddedResource Remove="Libraries\**" />
		<None Remove="Libraries\**" />
	</ItemGroup>
</Project>
